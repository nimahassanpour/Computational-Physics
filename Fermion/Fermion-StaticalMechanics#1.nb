     "mu$e$nucleus[r]"}}];
make$mue$plot[Z_?NumericQ, R_?NumericQ, mue$center_?NumericQ] := 
  Module[{}, mue = Evolve$mu$e[Z, R, mue$center];(*to be filled in*)
   Show[Plot[
     Evaluate[
      Evolve$mu$e[Z, R, mue$center] /. mue](*to be filled in*), {r, 
      r$center, R}, PlotLegends -> legend2]]];
